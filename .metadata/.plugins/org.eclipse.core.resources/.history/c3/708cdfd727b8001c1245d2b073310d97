//validacion kilometros

#include <stdio.h>

int valKilometros(int kilometros)
{

		while(kilometros < 0){
			printf("Error! Reingrese los kilometros: ");
			fflush(stdin);
			scanf("%d", &kilometros);
		}
		return kilometros;


}
