float debito(float precioVuelo, int descuento){
	float precioFinal;
	float porcDescuento;
	float descPrecio;
	porcDescuento = descuento/100;
	descPrecio = precioVuelo * porcDescuento;
	precioFinal = precioVuelo - descPrecio;
	return precioFinal;
}
